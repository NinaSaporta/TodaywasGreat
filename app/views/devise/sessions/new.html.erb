 <body>
 <div class="header">
      <%= link_to root_path do %> 
       <img class="twglogo" src="/images/twglogo.svg"> 
    <% end %>


    <nav class="nav">
     <img class="navSquares" src="/images/navSquares.svg">

       <div class="navMenuHide">
        <ul>
         <%= link_to root_path do %> 
          <li> Home
          </li>
         <% end %>
     
         <%= link_to '../about' do %> 
          <li> About 
          </li>
         <% end %>
   
          <%= link_to '../contact' do %> 
           <li> Contact
           </li>
          <% end %> 
        </ul>
      </div>
     </nav>

  <div class="loginPage">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <h2 class= "formTitle"> Existing User Login</h2>

   <div class="errorMessage">
      <%= devise_error_messages!  %>
   </div>
  

   <div class="newUserForm">
     <p><%= f.label :email %></p>
     <p><%= f.email_field :email, autofocus: true %></p>
   
     <p><%= f.label :password %></p>
     <p><%= f.password_field :password, autocomplete: "off" %></p>
   
     <% if devise_mapping.rememberable? -%>
     <p><%= f.check_box :remember_me %> <%= f.label :remember_me %></p>
     <% end -%>
   
     <p><%= f.submit "Sign in" %></p>
  

    <%= render "devise/shared/links" %>
    </div>
 </div>
<% end %>

</div>
   
 <div class="footer">
          <ul>
         <%= link_to "http://www.facebook.com/todaywasgreat" do %> 
          <li> <img class="fb" src="/images/fblogo.svg">
          </li>
         <% end %>

          <%= link_to "https://twitter.com/Today_was_great" do %> 
          <li> <img class="twitter" src="/images/twitterlogo.svg">
          </li>
         <% end %>

         <%= link_to "https://instagram.com/todaywasgreat" do %> 
          <li> <img class="instagram" src="/images/instagramlogo.svg">
          </li>
         <% end %>
        </ul>

      Copyright 2014 :: TodayWasGreat.com  :: Designed and Built by Nina Saporta
  </div> 
</body>